function solve(num1,num2,operator){
let firstNum = Number(num1);
let secondNum = Number(num2);
let result;
switch(operator){
    case '+':result = firstNum+secondNum;break;
    case '-':result = firstNum-secondNum;break;
    case '/':result = firstNum/secondNum;break;
    case '*':result = firstNum*secondNum;break;
    case '%':result = firstNum%secondNum;break;
    case '**':result = firstNum**secondNum;break;
}
console.log(result);
}

solve(5, 6, '+');
solve(3, 5.5, '*');
